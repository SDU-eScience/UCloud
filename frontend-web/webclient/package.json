{
    "name": "ucloud",
    "version": "2024.1.0-dev-22",
    "description": "UCloud",
    "type": "module",
    "author": "IMADA - eScience Center, SDU",
    "scripts": {
        "start": "vite",
        "start:local": "vite --mode local-dev",
        "start:compose": "vite --mode compose",
        "watch": "tsc --watch --noEmit",
        "build": "node scripts/update-version-file.js && vite build",
        "serve": "vite preview",
        "icons-clean": "cd app/ui-components/icons; rm -rf components index.ts",
        "icons-components": "cd app/ui-components/icons; svgr . --ext=tsx --filename-case camel -d components && rm -f components/index.tsx",
        "icons-index": "node scripts/create-icons-index.js"
    },
    "dependencies": {
        "@ginkgo-bioworks/react-json-schema-form-builder": "2.10.2",
        "@novnc/novnc": "1.4.0",
        "@reduxjs/toolkit": "2.1.0",
        "@rjsf/core": "4.2.3",
        "@svgr/cli": "5.5.0",
        "apexcharts": "3.45.2",
        "assert": "2.1.0",
        "date-fns": "3.3.1",
        "deepcopy": "2.1.0",
        "fast-deep-equal": "3.1.3",
        "fuse.js": "7.0.0",
        "jsrsasign": "10.8.6",
        "localforage": "1.10.0",
        "magic-bytes.js": "1.8.0",
        "monaco-editor": "0.45.0",
        "path-browserify": "1.0.1",
        "react": "18.2.0",
        "react-apexcharts": "1.4.1",
        "react-datepicker": "6.0.0",
        "react-dom": "18.2.0",
        "react-markdown": "9.0.1",
        "react-modal": "3.16.1",
        "react-redux": "8.1.3",
        "react-router": "6.22.0",
        "react-router-dom": "6.22.0",
        "react-syntax-highlighter": "15.5.0",
        "react-transition-group": "4.4.5",
        "react-virtualized-auto-sizer": "1.0.21",
        "react-window": "1.8.10",
        "redux": "4.2.1",
        "svgi": "1.1.2",
        "ua-parser-js": "1.0.37",
        "xterm": "5.3.0",
        "xterm-addon-fit": "0.8.0"
    },
    "devDependencies": {
        "@types/history": "5.0.0",
        "@types/json-schema": "7.0.15",
        "@types/jsrsasign": "10.5.12",
        "@types/react": "18.2.51",
        "@types/react-datepicker": "4.19.5",
        "@types/react-dom": "18.2.18",
        "@types/react-dropzone": "5.1.0",
        "@types/react-modal": "3.16.3",
        "@types/react-redux": "7.1.33",
        "@types/react-transition-group": "4.4.10",
        "@types/react-virtualized-auto-sizer": "1.0.4",
        "@types/react-window": "1.8.8",
        "@types/styled-system": "5.1.22",
        "@types/ua-parser-js": "0.7.39",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitejs/plugin-react-refresh": "1.3.6",
        "typescript": "5.3.3",
        "vite": "5.0.12"
    }
}
