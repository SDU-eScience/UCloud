    "jsonwebtoken": "8.5.1",
    "react": "16.9.0",
    "react-datepicker": "2.9.6",
    "react-dom": "16.9.0",
    "react-dropzone": "^10.1.8",
    "react-markdown": "^4.2.2",
    "react-modal": "^3.10.1",
    "react-redux": "^7.1.1",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "recharts": "^1.7.1",
    "redux": "4.0.4",
    "redux-responsive": "^4.3.8",
    "styled-components": "^4.3.2",
    "styled-system": "^5.1.1",
    "svgi": "^1.1.0",
    "xterm": "^4.0.0"
  },
