{
    "name": "ucloud",
    "version": "2024.1.85",
    "description": "UCloud",
    "type": "module",
    "author": "IMADA - eScience Center, SDU",
    "scripts": {
        "start": "vite",
        "start:local": "vite --mode local-dev",
        "start:compose": "vite --mode compose",
        "start:sandbox": "vite --mode sandbox",
        "watch": "tsc --watch --noEmit",
        "build": "node scripts/update-version-file.js && vite build",
        "serve": "vite preview",
        "icons-clean": "cd app/ui-components/icons; rm -rf components index.ts",
        "icons-components": "cd app/ui-components/icons; svgr . --ext=tsx --filename-case camel -d components && rm -f components/index.tsx",
        "icons-index": "node scripts/create-icons-index.js"
    },
    "dependencies": {
        "@novnc/novnc": "1.4.0",
        "@reduxjs/toolkit": "2.2.7",
        "@rjsf/core": "4.2.3",
        "@svgr/cli": "8.1.0",
        "apexcharts": "3.53.0",
        "assert": "2.1.0",
        "date-fns": "4.1.0",
        "deepcopy": "2.1.0",
        "fast-deep-equal": "3.1.3",
        "fuse.js": "7.0.0",
        "jsrsasign": "11.1.0",
        "localforage": "1.10.0",
        "magic-bytes.js": "1.10.0",
        "monaco-editor": "0.52.0",
        "react": "18.3.1",
        "react-apexcharts": "1.4.1",
        "react-datepicker": "6.9.0",
        "react-dom": "18.3.1",
        "react-markdown": "9.0.1",
        "react-modal": "3.16.1",
        "react-redux": "8.1.3",
        "react-router": "6.26.2",
        "react-router-dom": "6.26.2",
        "react-syntax-highlighter": "15.5.0",
        "react-transition-group": "4.4.5",
        "react-virtualized-auto-sizer": "^1.0.24",
        "react-window": "^1.8.10",
        "redux": "4.2.1",
        "rusha": "0.8.14",
        "ua-parser-js": "1.0.39",
        "xterm": "5.3.0",
        "xterm-addon-fit": "0.8.0"
    },
    "devDependencies": {
        "@types/history": "5.0.0",
        "@types/json-schema": "7.0.15",
        "@types/jsrsasign": "10.5.14",
        "@types/react": "18.3.9",
        "@types/react-datepicker": "6.2.0",
        "@types/react-dom": "18.3.0",
        "@types/react-modal": "3.16.3",
        "@types/react-redux": "7.1.34",
        "@types/react-transition-group": "4.4.11",
        "@types/react-window": "^1.8.8",
        "@types/rusha": "0.8.3",
        "@types/styled-system": "5.1.22",
        "@types/ua-parser-js": "0.7.39",
        "@vitejs/plugin-react": "4.3.1",
        "@vitejs/plugin-react-refresh": "1.3.6",
        "typescript": "5.6.2",
        "vite": "5.4.8"
    }
}
