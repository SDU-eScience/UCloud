{
    "name": "ucloud",
    "version": "2025.2.57",
    "description": "UCloud",
    "type": "module",
    "author": "IMADA - eScience Center, SDU",
    "scripts": {
        "start": "vite",
        "start:local": "vite --mode local-dev",
        "start:compose": "vite --mode compose",
        "start:sandbox": "vite --mode sandbox",
        "watch": "tsc --watch --noEmit",
        "build": "node scripts/update-version-file.js && vite build",
        "serve": "vite preview",
        "icons-clean": "cd app/ui-components/icons; rm -rf components index.ts",
        "icons-components": "cd app/ui-components/icons; svgr . --ext=tsx --filename-case camel -d components && rm -f components/index.tsx",
        "icons-index": "node scripts/create-icons-index.js"
    },
    "dependencies": {
        "@novnc/novnc": "1.5.0",
        "@observablehq/plot": "0.6.16",
        "@reduxjs/toolkit": "2.6.0",
        "@rjsf/core": "4.2.3",
        "@svgr/cli": "8.1.0",
        "apexcharts": "3.54.1",
        "assert": "2.1.0",
        "d3": "7.9.0",
        "date-fns": "4.1.0",
        "deepcopy": "2.1.0",
        "fast-deep-equal": "3.1.3",
        "fuse.js": "7.1.0",
        "jsrsasign": "11.1.0",
        "localforage": "1.10.0",
        "magic-bytes.js": "1.10.0",
        "monaco-editor": "0.52.2",
        "monaco-vim": "^0.4.2",
        "react": "19.0.0",
        "react-apexcharts": "1.4.4",
        "react-datepicker": "8.1.0",
        "react-dom": "19.0.0",
        "react-markdown": "10.0.1",
        "react-modal": "3.16.3",
        "react-redux": "9.2.0",
        "react-router": "7.2.0",
        "react-router-dom": "7.2.0",
        "react-syntax-highlighter": "15.6.1",
        "react-transition-group": "4.4.5",
        "react-virtualized-auto-sizer": "1.0.25",
        "react-window": "1.8.11",
        "redux": "5.0.1",
        "remark-gfm": "4.0.1",
        "rusha": "0.8.14",
        "ua-parser-js": "1.0.40",
        "xterm": "5.3.0",
        "xterm-addon-fit": "0.8.0",
        "yaml": "2.7.0"
    },
    "devDependencies": {
        "@types/d3": "7.4.3",
        "@types/history": "5.0.0",
        "@types/json-schema": "7.0.15",
        "@types/jsrsasign": "10.5.15",
        "@types/react": "19.0.10",
        "@types/react-datepicker": "7.0.0",
        "@types/react-dom": "19.0.4",
        "@types/react-modal": "3.16.3",
        "@types/react-redux": "7.1.34",
        "@types/react-transition-group": "4.4.12",
        "@types/react-window": "^1.8.8",
        "@types/rusha": "0.8.3",
        "@types/styled-system": "5.1.23",
        "@types/ua-parser-js": "0.7.39",
        "@vitejs/plugin-react": "4.3.4",
        "@vitejs/plugin-react-refresh": "1.3.6",
        "typescript": "5.8.2",
        "vite": "6.2.0"
    }
}
